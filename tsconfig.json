{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./web/*"],
      "@pages/*": ["./web/pages/*"],
      "@stores/*": ["./web/stores/*"],
      "@components/*": ["./web/components/*"]
    }
  }
}
